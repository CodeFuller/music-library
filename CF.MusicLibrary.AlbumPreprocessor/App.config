<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
  </startup>
  <connectionStrings>
    <add name="MediaMonkeyDB" connectionString="Provider=Microsoft.Jet.OLEDB.4.0; Data Source=d:\Dropbox\MediaMonkey.mdb" providerName="System.Data.OleDb" />
  </connectionStrings>
  <appSettings>
    <add key="AppDataPath" value="d:\Dropbox\prog\.data\AlbumPreprocessor"/>
    <add key="WorkshopDirectory" value="d:\Dropbox\prog\.data\AlbumPreprocessor\mp3"/>
    <add key="MediaMonkeyStorageRoot" value="\\localhost\music"/>
    <add key="LocalStorageRoot" value="d:\music.temp"/>
    <!-- Set to True if source song files should be moved to library (i.e. deleted from source location) -->
    <add key="MoveSongFilesToLibrary" value="False"/>
  </appSettings>
</configuration>
